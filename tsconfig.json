{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015.iterable",
    "noImplicitAny": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "pretty": true,
    "stripInternal": true,
    "noEmitHelpers": true,
    "outDir": "ts-out"
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
